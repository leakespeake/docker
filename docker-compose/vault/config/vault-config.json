{
  "storage": {
    "file": {
      "path": "vault/file"
    }
  },

  "listener": {
    "tcp":{
      "address": "vault-prd-01.int.leakespeake.com:8200",
      "tls_disable": 0,
      "tls_cert_file":"/vault/ssl/fullchain6.pem",
      "tls_key_file":"/vault/ssl/privkey6.pem",
      "tls_min_version": "tls12"
    }
  },

  "ui": true,
  "log_level": "info",
  "default_lease_ttl": "168h",
  "max_lease_ttl": "720h"
}
